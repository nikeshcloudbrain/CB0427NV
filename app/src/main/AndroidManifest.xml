        <activity android:name="com.creditscore.creditscorecheck.loan.activity.ScoreOffline3Activity"
            android:screenOrientation="portrait" />
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.ScoreOffline2Activity"
            android:screenOrientation="portrait" />
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.TipsTextActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.AdAppActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.QurekaInterActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.CalculaterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.CreditTips1Activity"
            android:screenOrientation="portrait" />
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.CreditScoreTipsActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.CheckCreditOfflineActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.ScoreOfflineActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.creditscore.creditscorecheck.loan.activity.CreditDescriptionActivity"
